<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	version="2.5">
	<servlet>
		<servlet-name>action</servlet-name>
		<servlet-class>com.ashigeru.lab.appengine.servlet.ActionServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>void</servlet-name>
		<servlet-class>com.ashigeru.lab.appengine.servlet.VoidServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>action</servlet-name>
		<url-pattern>/action/*</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>void</servlet-name>
		<url-pattern>/void</url-pattern>
	</servlet-mapping>
</web-app>
